algoritmo "Estrutura_repetição_REPITA"
// Função :
// Autor :
// Data : 24/03/2016
// Seção de Declarações
var
nome: literal
cont: inteiro
inicio
// Seção de Comandos
repita

   escreval("-----------------------")
   escreval("Digite um nome")
   leia(nome)
   escreval("Nome digitado foi: ", nome)

   cont <- cont + 1
ate cont = 5
fimalgoritmo
