algoritmo "semnome"
// Função :
// Autor :
// Data : 28/03/2016
// Seção de Declarações
var
cont: inteiro
num: inteiro

inicio
// Seção de Comandos

escreval ("Tabuada usando o ENQUANTO")
escreval ("Digite um número")
leia (num)

cont <- 1
enquanto cont <= 10 faca
   escreval (num, "x", cont, " = ", (num * cont))
   cont <- cont + 1

fimenquanto
fimalgoritmo
