algoritmo "semnome"
// Função :
// Autor :
// Data : 24/03/2016
// Seção de Declarações 
var
nome: literal
cont: inteiro
inicio
// Seção de Comandos
enquanto    cont <= 6        faca
  escreval("informe um nome: ")
  leia (nome)
  escreval ("O nome digitado foi: ", nome)
  
  cont <- cont +1

fimenquanto
fimalgoritmo
